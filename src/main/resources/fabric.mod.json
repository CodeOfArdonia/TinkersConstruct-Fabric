{
  "schemaVersion": 1,
  "id": "tconstruct",
  "provides": [
    "mantle"
  ],
  "version": "${version}",
  "name": "Supporoform",
  "description": "A little of this, a little of that, a lot of tinkering, and a lot of tools.",
  "authors": [
    "Slime Knights",
    "AlphaMode",
    "IAFEnvoy"
  ],
  "contact": {
    "sources": "https://github.com/CodeOfArdonia/TinkersConstruct-Fabric"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "slimeknights.mantle.Mantle",
      "slimeknights.tconstruct.TConstruct"
    ],
    "client": [
      "slimeknights.mantle.client.ClientEvents",
      "slimeknights.tconstruct.shared.TinkerClient"
    ],
    "fabric-datagen": [
      "slimeknights.mantle.MantleData",
      "slimeknights.tconstruct.TConstructData"
    ],
    "rei_client": [
      "slimeknights.mantle.plugin.rei.REIPlugin",
      "slimeknights.tconstruct.plugin.rei.REIPlugin"
    ],
    "rei_common": [
      "slimeknights.tconstruct.plugin.rei.REIPluginServer"
    ],
    "jei_mod_plugin": [
      "slimeknights.mantle.plugin.jei.JEIPlugin",
      "slimeknights.tconstruct.plugin.jei.JEIPlugin"
    ],
    "mixinsquared": [
      "slimeknights.tconstruct.StarSodiumFixer"
    ],
    "cardinal-components": [
      "slimeknights.mantle.util.OffhandCooldownTracker",
      "slimeknights.tconstruct.gadgets.capability.PiggybackCapability",
      "slimeknights.tconstruct.tools.logic.EquipmentChangeWatcher",
      "slimeknights.tconstruct.library.tools.capability.TinkerDataCapability",
      "slimeknights.tconstruct.library.tools.capability.PersistentDataCapability",
      "slimeknights.tconstruct.library.tools.capability.EntityModifierCapability"
    ]
  },
  "accessWidener": "tinkers.accesswidener",
  "custom": {
    "cardinal-components": [
      "mantle:offhand_cooldown",
      "tconstruct:piggyback",
      "tconstruct:equipment_watcher",
      "tconstruct:persistent_data",
      "tconstruct:modifier_data",
      "tconstruct:modifiers"
    ]
  },
  "mixins": [
    "mantle.mixins.json",
    "supporoform.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "porting_lib_accessors": ">=${port_lib_accessors_version}",
    "porting_lib_attributes": ">=${port_lib_attributes_version}",
    "porting_lib_base": ">=${port_lib_base_version}",
    "porting_lib_brewing": ">=${port_lib_brewing_version}",
    "porting_lib_client_events": ">=${port_lib_client_events_version}",
    "porting_lib_core": ">=${port_lib_core_version}",
    "porting_lib_config": ">=${port_lib_config_version}",
    "porting_lib_common": ">=${port_lib_common_version}",
    "porting_lib_data": ">=${port_lib_data_version}",
    "porting_lib_entity": ">=${port_lib_entity_version}",
    "porting_lib_extensions": ">=${port_lib_extensions_version}",
    "porting_lib_items": ">=${port_lib_items_version}",
    "porting_lib_fluids": ">=${port_lib_fluids_version}",
    "porting_lib_lazy_registration": ">=${port_lib_lazy_registration_version}",
    "porting_lib_loot": ">=${port_lib_loot_version}",
    "porting_lib_utility": ">=${port_lib_utility_version}",
    "porting_lib_models": ">=${port_lib_models_version}",
    "porting_lib_model_loader": ">=${port_lib_model_loader_version}",
    "porting_lib_networking": ">=${port_lib_common_version}",
    "porting_lib_tags": ">=${port_lib_tags_version}",
    "porting_lib_transfer": ">=${port_lib_transfer_version}",
    "porting_lib_tool_actions": ">=${port_lib_tool_actions_version}",
    "reach-entity-attributes": ">=${reach_entity_attributes_version}"
  }
}
